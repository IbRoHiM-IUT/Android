<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ibrokhimmovlonov.androidapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.SignIn" />
        <activity android:name=".Activities.SignUp" />
        <activity
            android:name=".Activities.HomePage"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme" />
        <activity android:name=".Activities.FoodList" />
        <activity android:name=".Activities.FoodInfo" />
        <activity android:name=".Activities.CartOrder" />
        <activity android:name=".Activities.OrderList" />
        <activity android:name=".Activities.Profile" />
        <activity android:name=".Activities.Favorities"></activity>
    </application>

</manifest>